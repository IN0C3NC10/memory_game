program JogoMemoria;

uses
  Vcl.Forms,
  untJogoMemória in 'untJogoMemória.pas' {frmJogo},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Auric');
  Application.CreateForm(TfrmJogo, frmJogo);
  Application.Run;
end.
